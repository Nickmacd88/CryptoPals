import binascii

def hexToBase(hex):
	
	binascii.unhexlify(hex)
	bytearray(binascii.unhexlify(hex))
	return binascii.b2a_base64(bytearray(binascii.unhexlify(hex))).decode()

print(hexToBase('49276d206b696c6c696e6720796f757220627261696e206c696b65206120706f69736f6e6f7573206d757368726f6f6d'))	
if hexToBase('49276d206b696c6c696e6720796f757220627261696e206c696b65206120706f69736f6e6f7573206d757368726f6f6d') == 'SSdtIGtpbGxpbmcgeW91ciBicmFpbiBsaWtlIGEgcG9pc29ub3VzIG11c2hyb29t':
	Print('Nice Work')
else:
	print('Try again')